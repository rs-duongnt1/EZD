{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "UMI_ENV=prod umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/lab": "^5.0.0-alpha.97",
    "@mui/material": "^5.10.3",
    "@umijs/plugins": "^4.0.18",
    "axios": "^0.27.2",
    "material-ui-popup-state": "^4.0.2",
    "react": "17.x",
    "react-dom": "17.x",
    "react-hook-form": "^7.34.2",
    "react-timeago": "^7.1.0",
    "sweetalert2": "^11.4.29",
    "umi": "4.0.18",
    "umi-plugin-convention-routes": "^1.0.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-timeago": "^4.1.3",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.33",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
