{
  "admin": {
    "listen": "0.0.0.0:2019"
  },
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [":443"],
          "routes": [
            {
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "transport": {
                    "protocol": "http",
                    "read_buffer_size": 4096
                  },
                  "upstreams": [
                    {
                      "dial": "application"
                    }
                  ]
                }
              ],
              "match": [
                {
                  "host": ["rsdn.site"]
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    }
  }
}
